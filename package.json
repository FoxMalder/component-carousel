  "license": "MIT",
  "version": "0.6.0",
  "description": "A micro, responsive, touch-enabled carousel.",
  "main":        "./dist/carousel.cjs.js",
  "jsnext:main": "./dist/carousel.es6.js",
  "browser":    "./dist/carousel.js",
  "repository": {
